# Fixing this code:
def get_container(product):
    matches = {
        "Bread": "Bag",
        "Milk": "bottle",
        "Beer": "bottle",
        "Eggs": "carton",
        "Cereals": "box",
        "Candy": "Plastic",
        "Cheese": None
    }
    return matches[product]


print(get_container("Bread"))
